{
  "expo": {
    "name": "bb-trax",
    "slug": "bb-trax",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "bbtrax",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.odddoc.bbtrax",
      "config": {
        "googleMapsApiKey": ""
      }
    },
    "android": {
      "config": {
        "googleMapsApiKey": ""
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.odddoc.bbtrax"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },

    "owner": "odd_doc"
  }
}
